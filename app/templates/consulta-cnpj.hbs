{{page-title 'Consulte dados de um CNPJ'}}

<HeaderWrapper::HeaderWrapper
  @cnpj={{this.cnpj}}
  @handleCnpj={{this.handleCnpj}}
  @queryCnpj={{this.queryCnpj}}
  @errorState={{this.errorState}}
/>

<main>
  {{#if this.loadedCnpj}}
    <CnpjData::CnpjData @cnpjData={{this.cnpjData}} />
  {{else}}
    <Content::Content />
  {{/if}}
</main>

<Footer::Footer />